<h2>Show Message</h2>

<ul>

  <li>
    <strong>User:</strong>
    <%= @message.user_id %>
  </li>

  <li>
    <strong>Titltle:</strong>
    <%= @message.titltle %>
  </li>

  <li>
    <strong>Post:</strong>
    <%= @message.post %>
  </li>

  <li>
    <strong>Likes:</strong>
    <%= @message.likes %>
  </li>

</ul>

<span><%= link "Edit", to: message_path(@conn, :edit, @message) %></span>
<span><%= link "Back", to: message_path(@conn, :index) %></span>
